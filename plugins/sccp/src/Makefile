include ${PROBE_ROOT}/probe.mk
OBJS= \
	MTP3Parser.o \
	SCCPParser.o \
	SCTPParser.o \
	SCCPGlobal.o \
	SCCPSessionManager.o \
	SCCPMessageBroker.o \
	SCCPConfig.o
	
all:${OBJS}

.cpp.o:

	${GCC} ${CFLAGS} -I/usr/include ${PROBE_INCLUDE} -c $<

clean:
	${RM} ${OBJS}
